@model SpecWheels.Models.Break.BreakModel

@{
    ViewBag.Title = "View Break";
}

<fieldset class="form-group">
    @Html.AntiForgeryToken()
    <h4>Break details</h4>
    <hr />
    @Html.ValidationSummary()

    <div class="form-group">
        @Html.LabelFor(a => a.Name, new { @class = "form-control-label" })
        @Html.TextBoxFor(a => a.Name, new { @class = "form-control disabled", @readonly = "readonly" })
    </div>

    <div class="form-group">
        @Html.LabelFor(a => a.Brand, new { @class = "form-control-label" })
        @Html.TextBoxFor(a => a.Brand, new { @class = "form-control disabled", @readonly = "readonly" })
    </div>

    <div class="form-group">
        @Html.LabelFor(a => a.Size, new { @class = "form-control-label" })
        @Html.TextBoxFor(a => a.Size, new { @class = "form-control disabled", @readonly = "readonly" })
    </div>

    <div class="form-group">
        @Html.LabelFor(a => a.Type, new { @class = "form-control-label" })
        @Html.TextBoxFor(a => a.Type, new { @class = "form-control disabled", @readonly = "readonly" })
    </div>

    

    @*<a class="btn btn-primary" href="@Url.Action("Edit", "User")">Edit User</a>
    <a class="btn btn-primary" href="@Url.Action("Inactivate", "User")">Inactivate User</a>*@

</fieldset>
    